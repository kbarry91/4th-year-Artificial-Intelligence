package ie.gmit.sw.ai;

import net.sourceforge.jFuzzyLogic.FIS;

/**
 * @author kevo-
 *
 */
public class Funding {
	FIS fis = FIS.load("fcl/funding.fcl", true); // Load and parse the FCL
	fis.chart(); // Display the linguistic variables and terms
	fis.setVariable("funding",60); // Apply a value to a varia
	ble
	fis.setVariable("staffing",14);fis.evaluate(); // Execute the fuzzy inference engine
	System.out.println(fis.getVariable("risk").getValue());
	// Output end resu
}
